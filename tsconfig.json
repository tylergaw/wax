{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/data/*"],
      "@util/*": ["src/util/*"],
      "@types": ["src/types.ts"],
      "@store": ["src/store.ts"]
    }
  }
}